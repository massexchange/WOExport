<table class="table table-striped">
    <thead>
        <tr>
            <th>Publication</th>
            <th>Average Price</th>
            <th>Monthly Change</th>
            <th>MTD Revenue</th>
        </tr>
    </thead>
    <tbody>
        {#publications}
            <tr>
                <td>{name}</td>
                <td>{price}</td>
                <td>{change}</td>
                <td>{monthRevenue}</td>
            </tr>
        {/publications}
    </tbody>
</table>
